{
  "version": 2,
  "builds": [
    {
      "src": "api/contact.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "api/contact.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}